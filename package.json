{
  "name": "front-client-marcasfuertes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "rm": "rm -rf node_modules .next  package-lock.json && npm install -f",
    "check:eslint": "eslint '*/**/*.{js,jsx,tsx}' --fix --max-warnings=0 --no-error-on-unmatched-pattern",
    "server": "nodemon node server.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "husky": "husky install",
    "lint": "next lint",
    "test": "echo \"Write tests!\" && exit 1",
    "lint:fix": "npm run lint -- --fix",
    "build:css": "node scripts/update-export-atoms.js",
    "build:globals": "node scripts/build-tokens.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  },
  "dependencies": {
    "cookies-next": "2.1.1",
    "husky": "^8.0.2",
    "js-cookie": "3.0.1",
    "lint-staged": "^13.0.3",
    "mouse-follower": "1.1.2",
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-spring": "^9.5.5",
    "styled-components": "5.3.6",
    "swiper": "8.4.4",
    "validate-branch-name": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.2.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.0"
  }
}
